{
	"manifest_version": 2,

	"name": "Reaction Saver",
	"description": "A Chrome extension for saving reaction images and GIFs.",
	"version": "1.0",

	"background": {
		"scripts": ["ReactionSaver.js"],
		"persistent": false
	},

	"permissions": [
		"contextMenus",
		"storage"
	],

	"icons": {
		"16": "icon-16.png"
	}
}
